        const val = "world hello this will be capitalized for each word";
        function wordsCaps(str) {
            const tempArr = [];
            let words = str.split(" ");
            words.forEach(word => {
                let temp = word.slice(0, 1).toUpperCase() + word.slice(1);
                tempArr.push(temp);
            });
            return tempArr.join(" ");
        }
        console.log(wordsCaps(val));
